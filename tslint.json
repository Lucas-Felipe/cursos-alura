{
  "extends": ["tslint:recommended", "tslint-plugin-prettier", "tslint-config-prettier"],
  "rules": {
    "no-console": false,
    "jsx-wrap-multiline": false,
    "max-line-length": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-var-requires": false,
    "indent": false,
    "no-empty": false,
    "no-shadowed-variable": false
  },
  "jsRules": {
    "no-console": false
  }

}
